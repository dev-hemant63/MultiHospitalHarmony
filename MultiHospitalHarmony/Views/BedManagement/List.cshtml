@model List<Bads>
@{
    if (Model != null)
    {
        int count = 0;
        foreach (var item in Model)
        {
            count++;
            <tr>
                <td>@count</td>
                <td>
                    <div class="btn-group mb-1">
                        <button class="btn btn-outline-dark btn-sm dropdown-toggle" type="button" data-bs-toggle="dropdown">
                        </button>
                        <div class="dropdown-menu">
                            <a class="dropdown-item" href="#" data-bs-toggle="modal" data-bs-target="#exampleModalCenter" onclick="getById('@item.Id')">Edit</a>
                            <a class="dropdown-item" href="#">Delete</a>
                        </div>
                    </div>
                </td>
                <td>@item.Name</td>
                <td>@item.Remark</td>
                <td>@item.Price</td>
                <td>@item.EntryAt</td>
            </tr>
        }
    }
}